{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "dom",
      "es5",
      "es2015"
    ],
    "target": "es5",
    "allowJs": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "removeComments": true,
    "sourceMap": false,
    //    "mapRoot": "typescripts",
    "baseUrl": "typescripts",
    "rootDir": "typescripts",
    "noUnusedLocals": false,
    "noImplicitAny": true,
    "outDir": "assets/Script"
  },
  "exclude": [
    "node_modules",
    "library",
    "local",
    "temp",
    "build",
    "settings"
  ],
  "include": [
    "typescripts/**/*"
  ]
}